<%= form_with model: vehicle do |form|%>
 <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
    <%= vehicle.errors.full_messages_for(:name).first do |message|%>
        <div><%= message%></div>
    <%end%>
</div><br>
<div>
    <%= form.label :weight %>
    <%= form.number_field :weight %>
    <%= vehicle.errors.full_messages_for(:weight).first do |message|%>
        <h6><%= message%></h6>
    <%end%>
</div><br>
<div>
<%=form.label :owned_by%>
<%= form.select :owned_by,["ISRO","NASA","CNSA","RFSA","ESA"],selected: "ISRO"%>
<% vehicle.errors.full_messages_for(:meal_type).each do |message| %>
      <div><%= message %></div>
 <% end %>
 </div><br>
<div class="actions">
<%= form.submit class: "button is sucess" %>
</div>
<%end%>


